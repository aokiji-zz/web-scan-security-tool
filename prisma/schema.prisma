// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Target {
  domain     String?
  externalIp String?
  internalIp String?
  url        String?
  Services   Service[]
  id         Int       @id() @default(autoincrement())
}

model Service {
  port     Int
  protocol String
  state    Boolean
  service  String
  version  String?
  product  String?
  id       Int     @id() @default(autoincrement())
  Target   Target @relation(fields: [targetId], references: [id])
  targetId Int
}

model TestModel {
  macAddress String?
  domain     String?
  externalIp String?
  internalIp String?
  url        String?
  port     Int
  protocol String
  state    Boolean
  service  String
  version  String?
  product  String?
  id       Int     @id() @default(autoincrement())
}